{
  "name": "netlify-monitor",
  "version": "0.0.3",
  "description": "A Chrome Extension to keep an eye on your Netlify builds.",
  "type": "module",
  "main": "index.js",
  "scripts": {
    "setup": "node setup.js",
    "test": "NODE_OPTIONS=--experimental-vm-modules npx jest --runInBand --testTimeout=60000",
    "dev": "browser-sync start --server --watch --startPath 'dev/index.html'",
    "dev-no-open": "browser-sync start --server --watch --no-open"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/matteocargnelutti/netlify-monitor.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/matteocargnelutti/netlify-monitor/issues"
  },
  "homepage": "https://github.com/matteocargnelutti/netlify-monitor#readme",
  "devDependencies": {
    "@types/chrome": "^0.0.200",
    "browser-sync": "^2.27.10",
    "dexie": "^3.2.2",
    "jest": "^29.3.1",
    "prompts": "^2.4.2",
    "puppeteer": "^19.2.2",
    "uuid": "^9.0.0",
    "zip-dir": "^2.0.0"
  }
}
